rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: This is a temporary, highly permissive rule for debugging purposes.
    // It allows anyone to read from and write to any part of your database.
    // This should be replaced with more specific rules before production.
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
